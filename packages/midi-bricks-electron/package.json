{
  "name": "midi-bricks-electron",
  "version": "0.0.0",
  "main": "public/electron.js",
  "homepage": "./",
  "description": "Midi Bricks for all!",
  "author": "TimSusa <timmsusa@googlemail.com>",
  "license": "ISC",
  "files": [
    "public",
    "build"
  ],
  "dependencies": {
    "electron-is-dev": "^0.3.0",
    "electron-window-state": "^5.0.1"
  },
  "build": {
    "appId": "tim.susa.midi-sliders.de",
    "mac": {
      "category": "your.app.category.type"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "files": [
      "build/**/*"
    ]
  },
  "devDependencies": {
    "midi-bricks": "latest",
    "@material-ui/core": "^3.6.2",
    "@material-ui/icons": "^3.0.1",
    "@material-ui/lab": "^3.0.0-alpha.25",
    "concurrently": "^3.6.0",
    "downshift": "^2.2.2",
    "electron": "^4.0.1",
    "electron-builder": "^20.38.5",
    "electron-publish": "^20.38.5",
    "react": "^16.6.3",
    "react-color": "^2.14.1",
    "react-dom": "^16.6.3",
    "react-grid-layout": "^0.16.6",
    "react-redux": "^6.0.0",
    "react-scripts": "^2.1.3",
    "react-sizeme": "^2.5.2",
    "react-gamepad": "^1.0.3",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.7",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.5.0",
    "redux-thunk": "^2.2.0",
    "tonal": "^2.0.0",
    "typeface-roboto": "0.0.54",
    "wait-for-localhost-cli": "^1.0.0",
    "webmidi": "^2.3.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "dev": "concurrently \"BROWSER=none react-scripts start\" \"wait-for-localhost 3000 && electron .\"",
    "build": "react-scripts build && build --mac --win",
    "test": "react-scripts test",
    "postinstall": "electron-builder install-app-deps && cp -R node_modules/midi-bricks/src src"
  }
}
