{
  "name": "midi-bricks-electron",
  "productName": "MIDI-Bricks",
  "version": "1.3.3",
  "main": "./build/electron.js",
  "homepage": ".",
  "repository": "https://github.com/TimSusa/midi-bricks-mono.git",
  "description": "Midi Bricks for all!",
  "author": "TimSusa <timmsusa@googlemail.com>",
  "copyright": "Copyright Â© 2019 Tim Susa",
  "license": "MIT",
  "files": [
    "public",
    "build"
  ],
  "build": {
    "appId": "com.electron.midi-bricks-electron",
    "asar": true,
    "asarUnpack": true,
    "releaseInfo": {
      "releaseNotesFile": "../../CHANGELOG.md"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icons/icon.ico",
      "publish": [
        "github"
      ]
    },
    "mac": {
      "category": "your.app.category.type",
      "icon": "build/icons/icon.icns",
      "publish": [
        "github"
      ]
    },
    "linux": {
      "target": "deb",
      "category": "midi.touch.device",
      "icon": "build/icons/icon.icns",
      "publish": [
        "github"
      ]
    },
    "files": [
      "build/**/*"
    ],
    "publish": {
      "provider": "github",
      "publishAutoUpdate": true
    }
  },
  "dependencies": {
    "electron-is-dev": "^1.0.1",
    "electron-log": "^3.0.5",
    "electron-updater": "^4.0.6",
    "electron-window-state": "5.0.3"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chai-roughly": "1.0.0",
    "concurrently": "4.1.0",
    "electron": "5.0.0",
    "electron-builder": "20.40.2",
    "electron-publish": "20.40.0",
    "midi-bricks": "latest",
    "mocha": "6.0.2",
    "react-scripts": "3.0.0",
    "rimraf": "^2.6.3",
    "spectron": "5.0.0",
    "wait-for-localhost-cli": "1.1.0",
    "wait-for-localhost": "*"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "dev": "REACT_APP_IS_WEB_MODE=false && yarn run clean && yarn cp-src && concurrently \"yarn cp-src-electron && BROWSER=none react-scripts start\" \"wait-for-localhost 3000 && electron .\"",
    "build": "REACT_APP_IS_WEB_MODE=false && yarn run clean && yarn build:web && yarn build:app",
    "build-ci": "REACT_APP_IS_WEB_MODE=false && yarn clean:dist && yarn build:web && yarn build:app-ci",
    "build:app": "yarn cp-src && yarn cp-src-electron && electron-builder",
    "build:app-ci": "yarn cp-src && yarn cp-src-electron && electron-builder --win --linux",
    "build:web": "yarn cp-src && react-scripts build",
    "pack": "yarn clean:dist && yarn cp-src && yarn cp-src-electron && electron-builder --dir --mac --win --linux",
    "release": "build --win --mac --linux -p always",
    "release-ci": "build --win --linux -p always",
    "test": "react-scripts test --colors",
    "test-e2e-ci": "concurrently \"BROWSER=none react-scripts start\" \"wait-for-localhost 3000 && CI=true mocha --colors\"",
    "test-e2e": "CI=true mocha --colors",
    "postinstall": "yarn clean && electron-builder install-app-deps && yarn cp-src",
    "cp-src": "cp -rf ../midi-bricks/src/ src",
    "cp-src-electron": "cp -rf ./public/*.js build",
    "clean": "yarn clean:dist && rimraf src",
    "clean:dist": "rimraf dist && rimraf build && mkdir build"
  },
  "gitHead": "9e11fc5940a251c30a631745481309caa181d393"
}
