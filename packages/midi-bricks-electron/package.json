{
  "name": "midi-bricks-electron",
  "productName": "MIDI Brikx",
  "version": "1.51.3",
  "main": "./build/electron.js",
  "homepage": ".",
  "repository": "https://github.com/TimSusa/midi-bricks-mono.git",
  "description": "Midi Bricks for all!",
  "author": "TimSusa <timmsusa@googlemail.com>",
  "copyright": "Copyright Â© 2019 Tim Susa",
  "license": "MIT",
  "files": [
    "public",
    "build"
  ],
  "build": {
    "appId": "com.electron.midi-bricks-electron",
    "asar": true,
    "asarUnpack": true,
    "win": {
      "target": "nsis",
      "icon": "build/icons/icon.ico",
      "publish": [
        "github"
      ]
    },
    "mac": {
      "category": "your.app.category.type",
      "icon": "build/icons/icon.icns",
      "publish": [
        "github"
      ]
    },
    "linux": {
      "target": "deb",
      "category": "midi.touch.device",
      "icon": "build/icons/icon.icns",
      "publish": [
        "github"
      ]
    },
    "files": [
      "build/**/*"
    ],
    "publish": {
      "provider": "github",
      "publishAutoUpdate": true
    }
  },
  "dependencies": {
    "electron-is-dev": "^1.0.1",
    "electron-log": "^3.0.5",
    "electron-publish": "20.39.0",
    "electron-updater": "^4.0.6",
    "electron-window-state": "5.0.3"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chai-roughly": "1.0.0",
    "concurrently": "4.1.0",
    "electron": "5.0.0",
    "electron-builder": "20.40.2",
    "midi-bricks": "^1.51.1",
    "mocha": "6.0.2",
    "react-scripts": "3.0.0",
    "rimraf": "2.6.3",
    "spectron": "5.0.0",
    "wait-for-localhost-cli": "1.1.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "dev": "concurrently \"BROWSER=none react-scripts start\" \"wait-for-localhost 3000 && cp -rf ./public/electron.js ./build\"  \"wait-for-localhost 3000 && electron .\"",
    "build": "yarn clean:dist && yarn build:web && yarn build:app",
    "build:app": "electron-builder --mac --win --linux",
    "build:web": "react-scripts build",
    "pack": "yarn clean:dist && electron-builder --dir --mac --win --linux",
    "release": "build --win --mac --linux -p always",
    "test": "react-scripts test --colors",
    "test-e2e-ci": "concurrently \"BROWSER=none react-scripts start\" \"wait-for-localhost 3000 && CI=true mocha --colors\"",
    "test-e2e": "CI=true mocha --colors",
    "postinstall": "yarn clean && electron-builder install-app-deps && cp -R node_modules/midi-bricks/src src",
    "clean": "yarn clean:dist && rimraf src",
    "clean:dist": "rimraf build && rimraf dist"
  },
  "gitHead": "aafdf0d3ef42b4bd010ada5d7fa3603505bf57df"
}
