{
  "name": "midi-bricks-electron",
  "productName": "MIDI Brikx",
  "version": "1.1.0",
  "main": "./build/electron.js",
  "homepage": ".",
  "repository": "https://github.com/TimSusa/midi-bricks-mono.git",
  "description": "Midi Bricks for all!",
  "author": "TimSusa <timmsusa@googlemail.com>",
  "copyright": "Copyright Â© 2019 Tim Susa",
  "license": "MIT",
  "files": [
    "public",
    "build"
  ],
  "build": {
    "appId": "com.electron.midi-bricks-electron",
    "asar": true,
    "asarUnpack": true,
    "mac": {
      "category": "your.app.category.type",
      "icon": "build/icon.icns",
      "publish": [
        "github"
      ]
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico",
      "publish": [
        "github"
      ]
    },
    "files": [
      "build/**/*"
    ],
    "publish": {
      "provider": "github",
      "token": "b46ba43be0bc07738c8575bfe8222dc14887f87a",
      "publishAutoUpdate": true,
      "releaseType": "release"
    }
  },
  "dependencies": {
    "electron-is-dev": "^1.0.1",
    "electron-log": "^3.0.5",
    "electron-publish": "20.39.0",
    "electron-updater": "^4.0.6",
    "electron-window-state": "5.0.3"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chai-roughly": "1.0.0",
    "concurrently": "4.1.0",
    "electron": "4.1.5",
    "electron-builder": "20.39.0",
    "midi-bricks": "^1.0.13",
    "mocha": "6.0.2",
    "react-scripts": "3.0.0",
    "rimraf": "2.6.3",
    "spectron": "5.0.0",
    "wait-for-localhost-cli": "1.1.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "dev": "concurrently \"BROWSER=none react-scripts start\" \"wait-for-localhost 3000 && electron .\"",
    "build": "react-scripts build",
    "pack": "build --mac --win --dir",
    "publish": "GH_TOKEN=b46ba43be0bc07738c8575bfe8222dc14887f87a build --mac --win -p always",
    "test": "react-scripts test --colors",
    "test-e2e-ci": "concurrently \"BROWSER=none react-scripts start\" \"wait-for-localhost 3000 && CI=true mocha --colors\"",
    "test-e2e": "CI=true mocha --colors",
    "postinstall": "electron-builder install-app-deps && rimraf src && cp -R node_modules/midi-bricks/src src",
    "clean": "rimraf build && rimraf dist && rimraf src && cp -R node_modules/midi-bricks/src src"
  },
  "gitHead": "7a8cfbeaa7eeb21f5a28e53626ce8552eee114b6"
}
