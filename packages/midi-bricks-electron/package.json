{
  "name": "midi-bricks-electron",
  "version": "0.12.23",
  "main": "./build/electron.js",
  "homepage": ".",
  "repository": "https://github.com/TimSusa/midi-bricks-mono/tree/master/packages/midi-bricks-electron",
  "description": "Midi Bricks for all!",
  "author": "TimSusa <timmsusa@googlemail.com>",
  "license": "MIT",
  "files": [
    "public",
    "build"
  ],
  "dependencies": {
    "electron-is-dev": "^1.0.1",
    "electron-window-state": "^5.0.1",
    "enzyme": "^3.8.0"
  },
  "build": {
    "appId": "tim.susa.midi-bricks.de",
    "mac": {
      "category": "your.app.category.type"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "files": [
      "build/**/*"
    ]
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "electron": "^4.0.1",
    "electron-builder": "^20.38.5",
    "electron-publish": "^20.38.5",
    "midi-bricks": "^0.12.23",
    "wait-for-localhost-cli": "^1.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "dev": "concurrently \"BROWSER=none react-scripts start\" \"wait-for-localhost 3000 && electron .\"",
    "build": "CI=true yarn test && yarn clean && react-scripts build && build --mac --win --linux",
    "test": "react-scripts test",
    "postinstall": "npm i --save-dev midi-bricks@latest && electron-builder install-app-deps && rimraf src && cp -R node_modules/midi-bricks/src src",
    "clean": "rimraf build && rimraf dist && rimraf src && cp -R node_modules/midi-bricks/src src"
  },
  "gitHead": "41af87a50f1b1a6c6eccb73909d55edba67d80bb"
}
