{
  "name": "midi-bricks-electron",
  "version": "0.12.25",
  "main": "./public/electron.js",
  "homepage": ".",
  "repository": "https://github.com/TimSusa/midi-bricks-mono/tree/master/packages/midi-bricks-electron",
  "description": "Midi Bricks for all!",
  "author": "TimSusa <timmsusa@googlemail.com>",
  "license": "MIT",
  "files": [
    "public",
    "build"
  ],
  "build": {
    "appId": "tim.susa.midi-bricks.de",
    "mac": {
      "category": "your.app.category.type"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "files": [
      "build/**/*"
    ]
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-roughly": "^1.0.0",
    "concurrently": "4.1.0",
    "electron": "4.0.2",
    "electron-builder": "20.38.5",
    "electron-is-dev": "^1.0.1",
    "electron-publish": "20.38.5",
    "electron-window-state": "^5.0.1",
    "midi-bricks": "^0.12.25",
    "mocha": "^5.2.0",
    "spectron": "^5.0.0",
    "wait-for-localhost-cli": "1.1.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "dev": "concurrently \"BROWSER=none react-scripts start\" \"wait-for-localhost 3000 && electron .\"",
    "build": "yarn clean && react-scripts build && build --mac --win --linux",
    "test": "react-scripts test",
    "test-e2e": "mocha",
    "postinstall": "npm i --save-dev midi-bricks@latest && electron-builder install-app-deps && rimraf src && cp -R node_modules/midi-bricks/src src",
    "clean": "rimraf build && rimraf dist && rimraf src && cp -R node_modules/midi-bricks/src src"
  },
  "gitHead": "1e1181b83d25f0e8f9f21feef501cb2270f90e92"
}
