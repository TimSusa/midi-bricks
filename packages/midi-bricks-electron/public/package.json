{
  "name": "midi-bricks-electron",
  "version": "1.0.13",
  "main": "./build/electron.js",
  "homepage": ".",
  "repository": "https://github.com/TimSusa/midi-bricks-mono/tree/master/packages/midi-bricks-electron",
  "description": "Midi Bricks for all!",
  "author": "TimSusa <timmsusa@googlemail.com>",
  "license": "MIT",
  "files": [
    "public",
    "build"
  ],
  "build": {
    "appId": "tim.susa.midi-bricks.de",
    "mac": {
      "category": "your.app.category.type"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "files": [
      "build/**/*"
    ]
  },
  "dependencies": {
    "electron-is-dev": "^1.0.1",
    "electron-publish": "20.39.0",
    "electron-window-state": "5.0.3"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chai-roughly": "1.0.0",
    "concurrently": "4.1.0",
    "electron": "4.1.4",
    "electron-builder": "20.39.0",
    "midi-bricks": "1.0.13",
    "mocha": "6.0.2",
    "react-scripts": "2.1.8",
    "rimraf": "2.6.3",
    "spectron": "5.0.0",
    "wait-for-localhost-cli": "1.1.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "dev": "concurrently \"BROWSER=none react-scripts start\" \"wait-for-localhost 3000 && electron .\"",
    "build": "react-scripts build && build --mac --win",
    "test": "react-scripts test --colors",
    "test-e2e-ci": "concurrently \"BROWSER=none react-scripts start\" \"wait-for-localhost 3000 && CI=true mocha --colors\"",
    "test-e2e": "CI=true mocha --colors",
    "postinstall": "electron-builder install-app-deps && rimraf src && cp -R node_modules/midi-bricks/src src && cp -f ./package.json ./public/",
    "clean": "rimraf build && rimraf dist && rimraf src && cp -R node_modules/midi-bricks/src src && cp -f ./package.json ./public/"
  },
  "gitHead": "7a8cfbeaa7eeb21f5a28e53626ce8552eee114b6"
}
