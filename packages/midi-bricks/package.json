{
  "name": "midi-bricks-electron",
  "productName": "MIDI-Bricks",
  "version": "1.3.41",
  "main": "./app/electron.js",
  "homepage": ".",
  "repository": "https://github.com/TimSusa/midi-bricks-mono.git",
  "description": "Midi Bricks for all!",
  "author": "TimSusa <timmsusa@googlemail.com>",
  "copyright": "Copyright Â© 2019 Tim Susa",
  "license": "MIT",
  "files": [
    "build/**/*",
    "node_modules/**/*"
  ],
  "build": {
    "appId": "com.electron.midi-bricks-electron",
    "asar": true,
    "asarUnpack": true,
    "releaseInfo": {
      "releaseNotesFile": "../../CHANGELOG.md"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icons/icon.ico",
      "publish": [
        "github"
      ]
    },
    "mac": {
      "category": "your.app.category.type",
      "icon": "build/icons/icon.icns",
      "publish": [
        "github"
      ]
    },
    "linux": {
      "target": "tar.gz",
      "category": "midi.touch.device",
      "icon": "build/icons/icon.icns",
      "publish": [
        "github"
      ]
    },
    "files": [
      "build/**/*"
    ],
    "publish": {
      "provider": "github",
      "publishAutoUpdate": true
    }
  },
  "dependencies": {
    "electron-is-dev": "1.2.0",
    "electron-log": "4.2.4",
    "electron-window-state": "5.0.3"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chai-roughly": "1.0.0",
    "concurrently": "5.3.0",
    "electron": "10.1.2",
    "electron-builder": "22.8.1",
    "electron-publish": "22.8.1",
    "mocha": "8.1.3",
    "rimraf": "3.0.2",
    "wait-for-localhost": "3.3.0",
    "wait-for-localhost-cli": "2.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "start": "export REACT_APP_IS_WEB_MODE=true && react-scripts start",
    "dev": "export REACT_APP_IS_WEB_MODE=false && yarn run clean && concurrently \"yarn cp-src-electron && BROWSER=none react-scripts start\" \"wait-for-localhost 3000 && electron .\"",
    "build": "export REACT_APP_IS_WEB_MODE=false && yarn run clean && yarn build:web && yarn build:app",
    "build-ci": "export REACT_APP_IS_WEB_MODE=false && yarn clean:dist && yarn build:web && yarn build:app-ci",
    "build:app": "yarn cp-src-electron && electron-builder -c.extraMetadata.main=build/app/electron.js --mac",
    "build:app-ci": "yarn cp-src-electron && electron-builder -c.extraMetadata.main=build/app/electron.js -mwl",
    "build:web": "export REACT_APP_IS_WEB_MODE=false && react-scripts build",
    "pack": "yarn clean:dist && yarn cp-src-electron && electron-builder --dir -mwl",
    "release": "electron-builder --mac -p always",
    "release-ci": "electron-builder -mwl -p always",
    "deploy": "export REACT_APP_IS_WEB_MODE=true && react-scripts build && cp now.json build/ && cd build && now --name midi-bricks && cd ..",
    "deploy-ci": "export REACT_APP_IS_WEB_MODE=true && react-scripts build && cp now.json build/ && cd build && now  --token=$ZEIT_TOKEN --name midi-bricks && cd ..",
    "test": "CI=true react-scripts test --colors",
    "analyze": "source-map-explorer build/static/js/main.*",
    "test-e2e-ci": "concurrently \"BROWSER=none react-scripts start\" \"wait-for-localhost 3000 && CI=true mocha --colors\"",
    "test-e2e": "CI=true mocha --colors",
    "postinstall": "yarn clean && electron-builder install-app-deps",
    "cp-src-electron": "mkdir build/app && cp -r ./app/ build/app",
    "clean": "yarn clean:dist",
    "clean:dist": "rimraf dist && rimraf build && mkdir build"
  },
  "gitHead": "7097b3906a1650a047b7e3c0aaf5731e19f23127"
}