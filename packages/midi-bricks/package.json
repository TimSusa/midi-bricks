{
  "name": "midi-bricks",
  "version": "1.3.40-alpha.6",
  "private": false,
  "homepage": "https://midi-bricks.timsusa.now.sh",
  "main": "./src/index.js",
  "license": "MIT",
  "repository": "https://github.com/TimSusa/midi-bricks-mono/tree/master/packages/midi-bricks",
  "scripts": {
    "start": "parcel ./src/index.html --out-dir build",
    "build": "REACT_APP_IS_WEB_MODE=true && parcel build ./src/index.html --out-dir build",
    "deploy": "REACT_APP_IS_WEB_MODE=true && yarn build && cp now.json build/ && cd build && now --name midi-bricks && cd ..",
    "deploy-ci": "REACT_APP_IS_WEB_MODE=true && yarn build && cp now.json build/ && cd build && now  --token=$ZEIT_TOKEN --name midi-bricks && cd ..",
    "test": "CI=true parcel test --colors",
    "analyze": "source-map-explorer build/static/js/main.*",
    "precommit": "yarn test"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "gitHead": "0f06bceadbb09f1c7dfac1012e8f98ac821a4bdd",
  "devDependencies": {
    "@babel/core": "^7.11.6"
  }
}
