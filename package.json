{
  "name": "midi-bricks-mono",
  "version": "0.24.12",
  "private": false,
  "license": "MIT",
  "repository": "https://github.com/TimSusa/midi-bricks-mono.git",
  "devDependencies": {
    "lerna": "3.13.1",
    "now": "14.3.0-canary.17",
    "react-scripts": "^2.1.8"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap && cd packages/midi-bricks-electron && yarn && cd ../..",
    "publish": "lerna publish",
    "deploy": "cd packages/midi-bricks/ && yarn deploy && cd ..",
    "build:midi-bricks": "cd packages/midi-bricks && yarn build && cd ..",
    "build:midi-bricks-electron": "cd packages/midi-bricks-electron && yarn && yarn build && cd ..",
    "dev:midi-bricks": "cd packages/midi-bricks && yarn start && cd ..",
    "dev:midi-bricks-electron": "cd packages/midi-bricks-electron && yarn dev && cd ..",
    "precommit": "yarn test && cd packages/midi-bricks/ && yarn test && cd .."
  }
}
